{
    "name": "yiisoft/yii-http-client",
    "description": "Yii Framework HTTP client extension",
    "keywords": ["yii", "http", "httpclient", "curl"],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-http-client/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-http-client"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "yiisoft/core": "^1.0"
    },
    "autoload": {
        "psr-4": {"yii\\httpclient\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"yiiunit\\httpclient\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
